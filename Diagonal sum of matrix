public class diago {
    public static void main(String[] args) {
        int arr[][] = {
            {1,2,3},
            {4,5,6},
            {7,8,9}
        };
        int primary=0,secondary=0;
        for(int i=0;i<arr.length;i++){
            primary+=arr[i][i];
            secondary+=arr[i][arr.length-i-1];
        }
        System.out.println(primary);
        System.out.println(secondary);            
    }
    }

