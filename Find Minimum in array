public class max {
    public static void main(String[] args) {
        int[] arr = {12,77,44,11,56,69};
        int min = arr[0];
        for(int i=1;i<arr.length;i++){
            if(arr[i]<min){
                min=arr[i];
            }
        }
        System.out.println("Largest element: " + min);
    }
}
